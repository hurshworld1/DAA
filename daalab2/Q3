#include <iostream>
using namespace std;
int main(){
    int n=3;
    int v[3]={100,60,120};
    int w[3]={20,10,40};
    int W=50;
    double total=0;
    total=total+60;
    W=W-10;
    total=total+100;
    W=W-20;
    total=total+(120.0/40)*W;
    cout<<"Maximum value = "<<total;
    return 0;
}
