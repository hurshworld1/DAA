#include <iostream>
#include <algorithm>
using namespace std;
int main(){
    int n=5;
    int at[5]={900,910,920,1100,1120};
    int dt[5]={940,1200,950,1130,1140};
    sort(at,at+n);
    sort(dt,dt+n);
    int plat=1;
    int maxPlat=1;
    int i=1,j=0;
    while(i<n && j<n){
        if(at[i]<=dt[j]){
            plat++;
            i++;
        }else{
            plat--;
            j++;
        }
        if(plat>maxPlat) maxPlat=plat;
    }
    cout<<"Minimum number of platforms required = "<<maxPlat;
    return 0;
}
